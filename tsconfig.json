{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES2017",
    "moduleResolution": "NodeNext",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "sourceMap": true
  }
}
